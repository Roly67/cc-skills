  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>CS Unit Test</Title>
      <Shortcut>csfact</Shortcut>
      <Description>xUnit Fact test method</Description>
      <Author>{CompanyName}</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>method</ID>
          <ToolTip>Method being tested</ToolTip>
          <Default>MethodName</Default>
        </Literal>
        <Literal>
          <ID>scenario</ID>
          <ToolTip>Test scenario</ToolTip>
          <Default>Scenario</Default>
        </Literal>
        <Literal>
          <ID>expected</ID>
          <ToolTip>Expected result</ToolTip>
          <Default>ExpectedResult</Default>
        </Literal>
        <Literal>
          <ID>description</ID>
          <ToolTip>Test description</ToolTip>
          <Default>Test description.</Default>
        </Literal>
      </Declarations>
      <Code Language="CSharp">
        <![CDATA[/// <summary>
/// $description$
/// </summary>
[Fact]
public async Task $method$_$scenario$_$expected$()
{
    // Arrange
    $end$

    // Act
    var result = await this.sut.$method$Async(CancellationToken.None);

    // Assert
    result.Should().NotBeNull();
}]]>
      </Code>
    </Snippet>
  </CodeSnippet>

