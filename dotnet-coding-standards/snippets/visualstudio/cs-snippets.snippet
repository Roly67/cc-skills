  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>CS Class</Title>
      <Shortcut>csclass</Shortcut>
      <Description>CS Class with copyright header and documentation</Description>
      <Author>{CompanyName}</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>classname</ID>
          <ToolTip>Class name</ToolTip>
          <Default>ClassName</Default>
        </Literal>
        <Literal>
          <ID>namespace</ID>
          <ToolTip>Namespace</ToolTip>
          <Default>CS.ProjectName</Default>
        </Literal>
        <Literal>
          <ID>description</ID>
          <ToolTip>Class description</ToolTip>
          <Default>Description of the class.</Default>
        </Literal>
      </Declarations>
      <Code Language="CSharp">
        <![CDATA[// <copyright file="$classname$.cs" company="{CompanyName}">
// © {CompanyName}
// </copyright>

namespace $namespace$;

/// <summary>
/// $description$
/// </summary>
public class $classname$
{
    $end$
}]]>
      </Code>
    </Snippet>
  </CodeSnippet>

