  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>CS Constructor with Annotations</Title>
      <Shortcut>csctorann</Shortcut>
      <Description>Constructor with JetBrains NotNull and guard clause</Description>
      <Author>{CompanyName}</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>classname</ID>
          <ToolTip>Class name</ToolTip>
          <Default>ClassName</Default>
        </Literal>
        <Literal>
          <ID>type</ID>
          <ToolTip>Parameter type</ToolTip>
          <Default>IService</Default>
        </Literal>
        <Literal>
          <ID>param</ID>
          <ToolTip>Parameter name</ToolTip>
          <Default>service</Default>
        </Literal>
        <Literal>
          <ID>paramdesc</ID>
          <ToolTip>Parameter description</ToolTip>
          <Default>The service instance</Default>
        </Literal>
      </Declarations>
      <Code Language="CSharp">
        <![CDATA[/// <summary>
/// Initializes a new instance of the <see cref="$classname$"/> class.
/// </summary>
