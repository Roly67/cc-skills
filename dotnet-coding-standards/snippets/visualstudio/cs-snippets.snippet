  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>CS Service</Title>
      <Shortcut>csservice</Shortcut>
      <Description>CS Service class with DI and logging</Description>
      <Author>{CompanyName}</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>name</ID>
          <ToolTip>Service name</ToolTip>
          <Default>ServiceName</Default>
        </Literal>
        <Literal>
          <ID>namespace</ID>
          <ToolTip>Namespace</ToolTip>
          <Default>CS.ProjectName.Application.Services</Default>
        </Literal>
        <Literal>
          <ID>description</ID>
          <ToolTip>Service description</ToolTip>
          <Default>Description of the service.</Default>
        </Literal>
      </Declarations>
      <Code Language="CSharp">
        <![CDATA[// <copyright file="$name$.cs" company="{CompanyName}">
// © {CompanyName}
// </copyright>

namespace $namespace$;

using Microsoft.Extensions.Logging;

/// <summary>
/// $description$
/// </summary>
public class $name$ : I$name$
{
    private readonly ILogger<$name$> logger;

    /// <summary>
    /// Initializes a new instance of the <see cref="$name$"/> class.
    /// </summary>
    /// <param name="logger">The logger instance.</param>
    public $name$(ILogger<$name$> logger)
    {
        this.logger = logger ?? throw new ArgumentNullException(nameof(logger));
    }

    $end$
}]]>
      </Code>
    </Snippet>
  </CodeSnippet>

